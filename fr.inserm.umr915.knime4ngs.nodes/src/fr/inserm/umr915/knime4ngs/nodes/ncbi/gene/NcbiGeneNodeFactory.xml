<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="../default.png" type="Source">
    <name>NCBI Gene</name>
    <shortDescription>NCBI Gene</shortDescription>
    
    <fullDescription>
        <intro>NCBI Gene. Example: <pre>##Symbol##[GENE] 9606[taxid]</pre></intro>
        <option name="QUERY">Entrez query. Use ##colname## to insert a value from the input table. e.g '<i>##Gene.name##[Symbol] 9606[TID]</i>'</option>
        <option name="LIMIT">limit number of results per query</option>
    </fullDescription>
    
    <ports>
        <inPort index="0" name="In-Port name">Input</inPort>
        <outPort index="0" name="Out-Port name">Output</outPort>
    </ports>
</knimeNode>
