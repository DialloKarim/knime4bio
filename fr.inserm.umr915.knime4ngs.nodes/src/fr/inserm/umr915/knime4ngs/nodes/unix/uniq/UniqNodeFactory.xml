<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="../default.png" type="Source">
    <name>Uniq</name>
    <shortDescription>Output the uniq rows from a sorted table</shortDescription>
    
    <fullDescription>
        <intro>Output the uniq rows from a <b>sorted</b> table. A <b>Column Resorter</b> Might be needed</intro>
        <option name="Columns">The columns to be observed</option>
        <option name="duplicate">only print duplicate rows.</option>
    </fullDescription>
    
    <ports>
    	<inPort index="0" name="Input0">Input 1</inPort>
        <outPort index="0" name="Output0">Output</outPort>
    </ports>
</knimeNode>
