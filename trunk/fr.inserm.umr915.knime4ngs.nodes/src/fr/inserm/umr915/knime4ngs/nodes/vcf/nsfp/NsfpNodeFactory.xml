<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="../default.png" type="Source">
    <name>dbNSFP</name>
    <shortDescription>dbNSFP: functional predictions from multiple algorithms for the comprehensive collection of human non-synonymous SNPs (NSs)</shortDescription>
    
    <fullDescription>
        <intro>Join data with NSFP 1.1 database (<a href="http://sites.google.com/site/jpopgen/dbNSFP">http://sites.google.com/site/jpopgen/dbNSFP</a> )</intro>
        <option name="Path">Path to <b>Zipped</b> version of NSFP (<b>NOT</b> 7-zip). Each entry must be sorted on chrom/position.</option>
        <option name="CHROM">VCF CHROM column</option>
        <option name="POS">VCF POS column</option>
        <option name="REF">VCF REF column</option>
        <option name="ALT">VCF ALT column</option>
    </fullDescription>
    
    <ports>
        <inPort index="0" name="In-Port name">VCF in</inPort>
        <outPort index="0" name="Accepted">VCF out</outPort>
    </ports>    

</knimeNode>
