<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="../default.png" type="Source">
    <name>GO</name>
    <shortDescription>Gene Ontology</shortDescription>
    
    <fullDescription>
        <intro>Call the QuickGO API to get the protein in the domain of a given GO ID</intro>
        <option name="Term">The GO ID. e.g: <b>GO:0003015</b></option>
        <option name="Taxon-ID">NCBI taxon ID . Human=9606</option>
    </fullDescription>
  
    <ports>
        <outPort index="0" name="QuickGO">QuickGO result</outPort>
    </ports>    

</knimeNode>
