<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE knimeNode PUBLIC "-//UNIKN//DTD KNIME Node 2.0//EN" "http://www.knime.org/Node.dtd">
<knimeNode icon="../default.png" type="Source">
    <name>Sample/Snp</name>
    <shortDescription>Gets the number of Samples having a given SNP</shortDescription>
    
    <fullDescription>
        <intro>
        This node gets the number of Samples having a given SNP.
        It adds an integer column for the number of columns per snp.
        Rows must be sorted by chom/pos/ref/alt/sample
        </intro>
        <option name="CHROM">The CHROM column</option>
        <option name="COL">The COL column</option>
        <option name="REF">The REF column</option>
        <option name="ALT">The ALT column</option>
        <option name="Sample">The Sample column</option>
    </fullDescription>
    
    <ports>
        <inPort index="0" name="input">input</inPort>
        <outPort index="0" name="ouptput">ouptput</outPort>
    </ports>    

</knimeNode>
